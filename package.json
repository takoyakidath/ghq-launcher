{
  "name": "ghq-launcher",
  "displayName": "ghq-launcher",
  "publisher": "takoyakidath",
  "icon": "images/icon.png",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ghq-extention.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
"activationEvents": [
  "onCommand:ghqStatusPicker.open",
  "onStartupFinished"
],

"contributes": {
  "commands": [
    {
      "command": "ghqStatusPicker.open",
      "title": "GHQ: Open Repository"
    }
  ],
  "configuration": {
    "title": "GHQ Status Picker",
    "properties": {
      "ghqStatusPicker.openInNewWindow": {
        "type": "boolean",
        "default": true
      },
      "ghqStatusPicker.maxMRU": {
        "type": "number",
        "default": 15,
        "minimum": 0,
        "maximum": 100
      }
    }
  }
}

}
